<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel='stylesheet' href='/stylesheets/style.css' />
	</head>
	<body>
		<h1><%= title %></h1>
		<%- include('includes/menu') -%>

		<div class="posts">
			<% posts.forEach((post) => { %>
				<div class="post">
					<h2><%= post.title %></h2>
					<img width="300" height="200" alt="img - <%= post.title %>" src="<%= post.image %>" />
					<p><%= post.content %></p>
					<b>Autor: <%= post.author %></b>
				</div>
			<% }); %>
		</div>
	</body>
</html>